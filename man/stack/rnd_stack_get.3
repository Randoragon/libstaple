.TH RND_STACK_GET 3 DATE "librnd-VERSION"
.SH NAME
rnd_stack_get, rnd_stack_getc, rnd_stack_gets, rnd_stack_geti, rnd_stack_getl,
rnd_stack_getsc, rnd_stack_getuc, rnd_stack_getus, rnd_stack_getui,
rnd_stack_getul, rnd_stack_getf, rnd_stack_getd, rnd_stack_getld \- look up
arbitrary element in a stack
.SH SYNOPSIS
.ad l
#include <rnd.h>
.sp
int
.BR rnd_stack_get "(const struct rnd_stack"
.RI * stack ,
size_t
.IR idx ,
